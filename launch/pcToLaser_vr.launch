<?xml version="1.0" ?>

<launch>

    <arg name="veh" default="husky2" />
    <arg name="cloud_in" default="velodyne1/velodyne_points" />
    <arg name="target_frame" default="base_link"/>

    <node ns="$(arg veh)" pkg="pointcloud_to_laserscan" type="pointcloud_to_laserscan_node" name="pointcloud_to_laserscan">

        <remap from="cloud_in" to="$(arg cloud_in)" />
        <remap from="scan" to="vr/scan" />
        <param name="target_frame" value="$(arg target_frame)"/>
        <rosparam>
            min_height: 0.2
            max_height: 1
            transform_tolerance: 0.01
            angle_min: -2.61799375
            angle_max: 2.61799375
            angle_increment: 0.017453
            scan_time: 0.1
            range_min: 0
            range_max: 50.0
            use_inf: False

            # Concurrency level, affects number of pointclouds queued for processing and number of threads used
            # 0 : Detect number of cores
            # 1 : Single threaded
            # 2->inf : Parallelism level
            concurrency_level: 0
        </rosparam>

    </node>
</launch>